class Solution {
    public int minDeletionSize(String[] strs) {
           int n=strs.length;
           int count=0;
            
             
             for(int i=0;i<strs[0].length();i++){
              for(int j=0;j<n-1;j++){
                  
                  if(strs[j+1].charAt(i)-strs[j].charAt(i)>=0){

                  }else{
                      count++;
                      break;
                  }
                    
                }   
                    
            }
            //System.out.println(count);
            
            return count;
            
    }
}
